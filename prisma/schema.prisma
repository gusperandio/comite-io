generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model DemandData {
  id              Int      @id @default(autoincrement()) // Auto-incrementing ID
  key             String  
  project         String
  type            String
  who             String
  supp            String
  demand          String
  analist         String
  aprov           String
  dev             String
  test            String
  date            String
  imp             String?
  show            Boolean
  usersApproved   String?
  usersRefused    String?
  usersIndifferent String?
}
